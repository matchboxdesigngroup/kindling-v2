{
    "name": "matchboxdesigngroup/kindling",
    "description": "Shared packages for the kindling base theme.",
    "keywords": [],
    "license": "GPL-3.0",
    "type": "project",
    "require": {
        "tightenco/collect": "^5.4",
        "nesbot/carbon": "^1.22",
        "fzaninotto/faker": "^1.6"
    },
    "autoload": {
        "psr-4": {
            "Kindling\\PostTypes\\": "wp-content/plugins/kindling-post-types/src",
            "Kindling\\Theme\\": "wp-content/themes/kindling/src"
        },
        "files": [
            "wp-content/plugins/kindling-post-types/libs/loader.php",
            "wp-content/plugins/kindling-development/libs/loader.php"
        ]
    },
    "autoload-dev": {
        "Kindling\\Theme\\Tests\\": "wp-content/themes/kindling/tests"
    },
    "config": {
        "preferred-install": "dist"
    }
}
